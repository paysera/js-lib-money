{
  "name": "@paysera/money",
  "version": "1.2.1",
  "description": "A library for working with Money",
  "main": "dist/main.js",
  "module": "es/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config demo/webpack.config.js",
    "build": "npm run build:umd && npm run build:es",
    "build:umd": "webpack --mode production --config webpack.config.js",
    "build:es": "cross-env BABEL_ENV=es babel ./src --out-dir es --copy-files --ignore \"src/**/__tests__\"",
    "clean": "rimraf dist es",
    "test": "jest --config jest.config.js --no-cache",
    "lint": "eslint --ext .js,.jsx .",
    "prepublishOnly": "npm run test && npm run clean && npm run build"
  },
  "files": [
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "dist",
    "es",
    "src"
  ],
  "author": "Paysera",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/paysera/js-lib-money.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.11.1"
  },
  "dependencies": {
    "bignumber.js": "^7.2.1",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@paysera/eslint-config": "^1.0.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.19.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.4",
    "jest": "^26.1.0",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.0",
    "webpack-cli": "^4.3.0",
    "webpack-node-externals": "^2.5.0"
  }
}
